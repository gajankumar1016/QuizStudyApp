<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.illinois.finalproject">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CourseDisplayImplementation.ViewCoursesActivity" />
        <activity android:name=".CourseDisplayImplementation.AddCourseActivity" />
        <activity android:name=".UnitDisplayImplementation.ViewUnitsActivity" />
        <activity android:name=".UnitDisplayImplementation.AddUnitActivity" />
        <activity android:name=".ProblemDisplayImplementation.ViewProblemsActivity" />
        <activity android:name=".ProblemDisplayImplementation.AddProblemActivity" />
        <activity android:name=".QuizQuestionActivity" />
        <activity android:name=".RevealSolutionActivity" />
        <activity android:name=".ProblemDisplayImplementation.ProblemDetailActivity"></activity>
    </application>

</manifest>