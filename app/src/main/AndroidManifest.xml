<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.illinois.finalproject">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".coursedisplayimplementation.ViewCoursesActivity" />
        <activity android:name=".coursedisplayimplementation.AddCourseActivity" />
        <activity android:name=".unitdisplayimplementation.ViewUnitsActivity" />
        <activity android:name=".unitdisplayimplementation.AddUnitActivity" />
        <activity android:name=".problemdisplayimplementation.ViewProblemsActivity"></activity>
    </application>

</manifest>